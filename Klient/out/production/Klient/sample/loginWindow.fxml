<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerLogin">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="loginButton" layoutX="289.0" layoutY="263.0" mnemonicParsing="false" onAction="#onActionLogin" text="Sign in" />
            <TextField fx:id="loginText" layoutX="228.0" layoutY="94.0" />
            <Button fx:id="registerButton" layoutX="259.0" layoutY="318.0" mnemonicParsing="false" onAction="#onActionRegister" prefHeight="31.0" prefWidth="126.0" text="Register" />
            <Text fx:id="nickText" layoutX="286.0" layoutY="83.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nickname" />
            <TextField fx:id="passField" layoutX="228.0" layoutY="200.0" />
            <Text fx:id="passText" layoutX="291.0" layoutY="180.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
      </children>
    </AnchorPane>
  </children>
</VBox>
