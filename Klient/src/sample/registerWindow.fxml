            <CheckBox fx:id="scienceCheck" layoutX="400.0" layoutY="322.0" mnemonicParsing="false" text="Science" />
            <Text fx:id="topicText" layoutX="273.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Choose topics" textAlignment="CENTER" />
            <Button fx:id="registerButton" layoutX="552.0" layoutY="355.0" mnemonicParsing="false" onAction="#onActionRegister" text="Register" />
            <Text fx:id="passText" layoutX="433.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
            <TextField fx:id="passField" layoutX="385.0" layoutY="35.0" />
            <TextField fx:id="ipField" layoutX="30.0" layoutY="355.0" />
            <Text layoutX="14.0" layoutY="376.0" strokeType="OUTSIDE" strokeWidth="0.0" text="IP:" />
         </children>
    </AnchorPane>
  </children>
</VBox>
