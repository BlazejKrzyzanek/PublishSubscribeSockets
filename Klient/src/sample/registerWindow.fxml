            <CheckBox fx:id="newsCheck" layoutX="120.0" layoutY="109.0" mnemonicParsing="false" prefWidth="93.0" text="News" />
            <CheckBox fx:id="socialCheck" layoutX="120.0" layoutY="158.0" mnemonicParsing="false" text="Social media" />
            <CheckBox fx:id="technoCheck" layoutX="120.0" layoutY="212.0" mnemonicParsing="false" text="Technology" />
            <CheckBox fx:id="animalsCheck" layoutX="120.0" layoutY="267.0" mnemonicParsing="false" text="Animals" />
            <CheckBox fx:id="musicCheck" layoutX="120.0" layoutY="322.0" mnemonicParsing="false" text="Music" />
            <CheckBox fx:id="filmCheck" layoutX="400.0" layoutY="109.0" mnemonicParsing="false" text="Film" />
            <CheckBox fx:id="lifeCheck" layoutX="400.0" layoutY="158.0" mnemonicParsing="false" text="Lifestyle" />
